<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.report.reportapp.RegisterController">
   <children>
      <VBox layoutX="200.0" layoutY="100.0" prefHeight="300.0" prefWidth="440.0">
