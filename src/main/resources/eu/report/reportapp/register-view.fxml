               <VBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
            </Label>
            <TextField promptText="User Name">
               <VBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Label prefHeight="20.0" prefWidth="143.0" text="Enter Password">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
            <TextField promptText="User Name" />
            <Label prefHeight="20.0" prefWidth="143.0" text="Re-enter Password">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
            <TextField promptText="User Name" />
         </children>
      </VBox>
      <ImageView fitHeight="400.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/bg_yellow_200x400.png" />
